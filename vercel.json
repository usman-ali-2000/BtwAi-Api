{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "crons": [
      {
        "path": "/api/cron.js",
        "schedule": "* * * * *"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
    
  }
  